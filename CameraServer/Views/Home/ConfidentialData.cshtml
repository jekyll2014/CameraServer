@{
    ViewData["Title"] = "Confidential Data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>WebUserSettings data</h2>

@if (User.Identity?.IsAuthenticated ?? false)
{
    <table class="table table-bordered">
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
}